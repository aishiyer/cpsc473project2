<!-- Info when sign-in -->
{{#if session.isAuthenticated}}

<div class="jumbotron">
  <div class="container">
    <h1>Welcome {{currentUser.user.email}}!</h1>

  </div>
</div>

<div class="container">


  <!-- footer -->
  <footer>
    <p>Group Members: Aishwarya Iyer, Joshua Marvel, Tevisophea Heng, Kimberly Nguyen, Briana Hernandez</p>

  </footer>
</div>

{{else}}
<div class='page-header'>
  <h1>You must be signed in to use this app</h1>
</div>
<div class='list-group'>
  {{#link-to 'sign-up' class='list-group-item'}}
    Sign Up
    {{/link-to}}
  {{#link-to 'sign-in' class='list-group-item'}}
    Sign In
    {{/link-to}}
  </div>
  {{/if}}
<!-- /container -->
